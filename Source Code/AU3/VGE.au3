Func WRITE_SCRIPT()
	If $REZOK = 0 Then Return
	$QWE = MsgBox(4, "", "Are you sure you wish to save the file now?")
	If $QWE = 6 Then
		$FILE_NAME = FileSaveDialog("Save As...", "@DesktopDir", "(*.au3)", 16)
		If @OSVersion = "WIN_2000" Then
			$BAR = 25
		Else
			$BAR = 30
		EndIf
		$S = 'GuiCreate("' & $TITLE & '",' & $DEM0 - 6 & "," & $DEM1 - $BAR & "," & $POS0 & "," & $POS1 & ")"
		If $BN > 0 Then
			$QWE = 0
			Do
				$QWE = $QWE + 1
				$ASD = $QWE * 10
				$S = $S & @CRLF & "$button" & $QWE & '=GuiCtrlCreateButton("' & $B[$ASD + 6] & '",' & $B[$ASD + 1] & "," & $B[$ASD + 2] & "," & $B[$ASD + 3] & "," & $B[$ASD + 4] & ")"
			Until $QWE = $BN
		EndIf
		If $IN > 0 Then
			$QWE = 0
			Do
				$QWE = $QWE + 1
				$ASD = $QWE * 10
				$S = $S & @CRLF & "$input" & $QWE & '=GuiCtrlCreateInput("' & $I[$ASD + 6] & '",' & $I[$ASD + 1] & "," & $I[$ASD + 2] & "," & $I[$ASD + 3] & "," & $I[$ASD + 4] & ")"
			Until $QWE = $IN
		EndIf
		If $LN > 0 Then
			$QWE = 0
			Do
				$QWE = $QWE + 1
				$ASD = $QWE * 10
				$S = $S & @CRLF & "$label" & $QWE & '=GuiCtrlCreateLabel("' & $L[$ASD + 6] & '",' & $L[$ASD + 1] & "," & $L[$ASD + 2] & "," & $L[$ASD + 3] & "," & $L[$ASD + 4] & ")"
			Until $QWE = $LN
		EndIf
		$S = $S & @CRLF & "GuiSetState()" & @CRLF & @CRLF & "While 1" & @CRLF & "$msg=GuiGetMsg()" & @CRLF & "If $msg=-3 Then Exit"
		If $BN > 0 Then
			$QWE = 0
			Do
				$QWE = $QWE + 1
				$S = $S & @CRLF & "If $msg=$button" & $QWE & " Then button" & $QWE & "()"
			Until $QWE = $BN
		EndIf
		$S = $S & @CRLF & "Wend"
		If $BN > 0 Then
			$S = $S & @CRLF & @CRLF & @CRLF
			$QWE = 0
			Do
				$QWE = $QWE + 1
				$S = $S & @CRLF & @CRLF & "Func button" & $QWE & "()" & @CRLF & "EndFunc"
			Until $QWE = $BN
		EndIf
		If StringRight($FILE_NAME, 4) = ".au3" Then
			$FILE_EXT = ""
			If StringLen($FILE_NAME) = 4 Then
				MsgBox(0, "Save", "No file name was given")
				MAIN()
			EndIf
		Else
			$FILE_EXT = ".au3"
			If StringLen($FILE_NAME) = 0 Then
				MsgBox(0, "Save", "No file name was given")
				MAIN()
			EndIf
		EndIf
		FileDelete($FILE_NAME & $FILE_EXT)
		FileWrite($FILE_NAME & $FILE_EXT, $S)
		MsgBox(0, "", "Finished.")
		CLOSE()
	EndIf
EndFunc