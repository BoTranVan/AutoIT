Func MAIN()
	GUISetState(5, $WINHAN)
	GUISetState(5, $WINHAN1)
	While 1
		$MSG = GUIGetMsg()
		If $MSG = -3 Then CLOSE()
		If $MSG = $DELINPUT Then DELINPUT()
		If $MSG = $DELBUTTON Then DELBUTTON()
		If $MSG = $DELLABEL Then DELLABEL()
		If $MSG = $NEWBUTTON Then MAKEBUTTON()
		If $MSG = $NEWLABEL Then MAKELABEL()
		If $MSG = $NEWINPUT Then MAKEINPUT()
		If $MSG = $EXPORT Then WRITE_SCRIPT()
		If $MSG = $NEW Then MAKEWINDOW()
		If $MSG = $ABOUT Then ABOUT()
		$QWE = WinGetPos($TITLE)
		$POS0 = $QWE[0]
		$POS1 = $QWE[1]
		$DEM0 = $QWE[2]
		$DEM1 = $QWE[3]
	WEnd
