Func MAKEWINDOW()
	RESET()
	While 1
		$TITLE = InputBox("Setup", "Step 1.  Set a title" & @CRLF & "Step 2.  Click OK to create the window" & @CRLF & "Step 3.  Click to position the window" & @CRLF & "Step 4.  Click again to Size the window")
		If $TITLE = "Control pannel" Or $TITLE = "Control panne" Or $TITLE = "Control pann" Or $TITLE = "Control pan" Or $TITLE = "Control pa" Or $TITLE = "Control p" Or $TITLE = "Control " Or $TITLE = "Control" Or $TITLE = "Contro" Or $TITLE = "Contr" Or $TITLE = "Cont" Or $TITLE = "Con" Or $TITLE = "Co" Or $TITLE = "C" Then
			MsgBox(0, "Error", 'Creating a window that begins with part or all of "Control pannel" causes errors.' & @CRLF & "Try just adding something on the end or in the middle and change it later.")
		Else
			If $TITLE = "" Then
				MAIN()
			Else
				$REZOK = 1
				ExitLoop
			EndIf
		EndIf
	WEnd
	$WINHAN = GUICreate($TITLE, 200, 200, -100, -100)
	GUISetState()
	_MOUSETRAP(200, 220, 10000, 10000)
	While 1
		$QWE = MouseGetPos()
		WinMove($TITLE, "", $QWE[0] - 200, $QWE[1] - 220)
		$QWE = GUIGetCursorInfo($WINHAN)
		If $QWE = 0 Then
			WinActivate($WINHAN)
		Else
			If $QWE[2] Then ExitLoop
		EndIf
	WEnd
	$QWE = WinGetPos($TITLE)
	$POS0 = $QWE[0]
	$POS1 = $QWE[1]
	MouseUp("main")
	_MOUSETRAP($POS0 + 80, $POS1 + 30, 10000, 10000)
	While 1
		$QWE = MouseGetPos()
		WinMove($TITLE, "", $POS0, $POS1, $QWE[0] - $POS0 + 6, $QWE[1] - $POS1 + 5)
		$QWE = GUIGetCursorInfo($WINHAN)
		If $QWE = 0 Then
			WinActivate($WINHAN)
		Else
			If $QWE[2] Then ExitLoop
		EndIf
	WEnd
	_MOUSETRAP()
EndFunc


