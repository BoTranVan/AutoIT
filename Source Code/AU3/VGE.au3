	$TBUFFER = DllStructCreate("char Text[256]", DllStructGetData($TPTR, "Buffer"))
	$SSID = DllStructGetData($TBUFFER, "Text")
	_WINAPI_LOCALFREE(DllStructGetData($TPTR, "Buffer"))
	Return $SSID
EndFunc


